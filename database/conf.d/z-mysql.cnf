[mysql]

[mysqld]
table_open_cache = 4000
thread_cache_size = 80
host_cache_size = 1000
read_buffer_size = 2M
read_rnd_buffer_size = 1M
join_buffer_size = 2M
sort_buffer_size = 2M
innodb_buffer_pool_size = 1G
performance_schema = OFF
max_heap_table_size = 32M
tmp_table_size = 32M
pid_file = /usr/local/mysql/data/mysqld.pid
